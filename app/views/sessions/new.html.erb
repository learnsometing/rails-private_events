<%= provide(:title, 'Log in')%>

<h1>Log in</h1>
<div class='row'>
    <div class='col-md-4 offset-md-4'>
        <%= form_for(:session, url: login_path) do |f|%>
            <%= f.label :name %>
            <%= f.text_field :name, class: 'form-control' %></br>
            <%= f.submit 'Log in', class: 'btn btn-dark' %>
        <% end %>
    </div>
</div>